         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="it.ltc.logica.container"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="lifeCycleURI"
               value="bundleclass://it.ltc.logica.container/it.ltc.logica.container.lifecycle.Manager">
         </property>
         <property
               name="applicationCSS"
               value="platform:/plugin/it.ltc.logica.container/css/default.css">
