package it.ltc.logica.trasporti.calcolo.algoritmi;

public enum TipoAlgoritmo {
	
	FISSO("FIS", "Fisso"),
	PERCENTUALE("PER", "Percentuale"),
	PROPORZIONALE("PRO", "Proporzionale"),
	SCAGLIONI("SCA", "A Scaglioni"),
	SCAGLIONI_RIPETUTI("SCR", "A Scaglioni Ripetuti");
	
	private final String codifica;
	private final String nome;
	
	private TipoAlgoritmo(String codifica, String nome) {
		this.codifica = codifica;
		this.nome = nome;
	}
	
	public String toString() {
		return nome;
	}
 
	public String getCodifica() {
		return codifica;
	}

	public String getNome() {
		return nome;
	}

}
