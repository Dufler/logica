package it.ltc.logica.database.model.sqlserver.cliente;

import java.util.Date;

import it.ltc.logica.database.orm.ColumnList;
import it.ltc.logica.database.orm.Entity;

public class RighiOrdine extends Entity {

	public static final String nomeTabella = "dbo.RighiOrdine";
	private static final ColumnList variabili = new ColumnList();
	
	private Integer id;
	private String nrLista;
	private Date dataOrdine;
	private String numerata;
	private String codiceArticolo;
	private String taglia;
	private Integer posizione;
	private Integer qtaSpedizione;
	private Integer qtaDaUbicare;
	private String descrizione;
	private Integer nrRigo;
	private Integer nrOrdine;
	private Integer idDestina;
	private String barraEAN;
	private String BarraUPC;
	private String idUnicoArt;
	private String magazzino;
	private String ragstampe;
	private String tipoOrdine;
	private String colore;
	
	@Override
	public String getNomeTabella() {
		return nomeTabella;
	}

	@Override
	public ColumnList getVariabili() {
		if (variabili.isEmpty()) {
			variabili.addAutoincrement("idRigoOrdine", "id");
			variabili.addString("NrLista", "nrLista");
			variabili.addDate("DataOrdine", "dataOrdine");
			variabili.addString("Numerata", "numerata");
			variabili.addString("CodiceArticolo", "codiceArticolo");
			variabili.addString("Taglia", "taglia");
			variabili.addInt("Posizione", "posizione");
			variabili.addInt("QtaSpedizione", "qtaSpedizione");
			variabili.addInt("Qtadaubicare", "qtaDaUbicare");
			variabili.addString("Descrizione", "descrizione");
			variabili.addInt("NrRigo", "nrRigo");
			variabili.addInt("NrOrdine", "nrOrdine");
			variabili.addInt("IdDestina", "idDestina");
			variabili.addString("BarraEAN", "barraEAN");
			variabili.addString("BarraUPC", "barraUPC");
			variabili.addString("IdUnicoArt", "idUnicoArt");
			variabili.addString("Magazzino", "magazzino");
			variabili.addString("Ragstampe1", "ragstampe");
			variabili.addString("Tipoord", "tipoOrdine");
			variabili.addString("Colore", "colore");
		}
		return variabili;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getNrLista() {
		return nrLista;
	}

	public void setNrLista(String nrLista) {
		this.nrLista = nrLista;
	}

	public Date getDataOrdine() {
		return dataOrdine;
	}

	public void setDataOrdine(Date dataOrdine) {
		this.dataOrdine = dataOrdine;
	}

	public String getNumerata() {
		return numerata;
	}

	public void setNumerata(String numerata) {
		this.numerata = numerata;
	}

	public String getCodiceArticolo() {
		return codiceArticolo;
	}

	public void setCodiceArticolo(String codiceArticolo) {
		this.codiceArticolo = codiceArticolo;
	}

	public String getTaglia() {
		return taglia;
	}

	public void setTaglia(String taglia) {
		this.taglia = taglia;
	}

	public Integer getPosizione() {
		return posizione;
	}

	public void setPosizione(Integer posizione) {
		this.posizione = posizione;
	}

	public Integer getQtaSpedizione() {
		return qtaSpedizione;
	}

	public void setQtaSpedizione(Integer qtaSpedizione) {
		this.qtaSpedizione = qtaSpedizione;
	}

	public Integer getQtaDaUbicare() {
		return qtaDaUbicare;
	}

	public void setQtaDaUbicare(Integer qtaDaUbicare) {
		this.qtaDaUbicare = qtaDaUbicare;
	}

	public String getDescrizione() {
		return descrizione;
	}

	public void setDescrizione(String descrizione) {
		this.descrizione = descrizione;
	}

	public Integer getNrRigo() {
		return nrRigo;
	}

	public void setNrRigo(Integer nrRigo) {
		this.nrRigo = nrRigo;
	}

	public Integer getNrOrdine() {
		return nrOrdine;
	}

	public void setNrOrdine(Integer nrOrdine) {
		this.nrOrdine = nrOrdine;
	}

	public Integer getIdDestina() {
		return idDestina;
	}

	public void setIdDestina(Integer idDestina) {
		this.idDestina = idDestina;
	}

	public String getBarraEAN() {
		return barraEAN;
	}

	public void setBarraEAN(String barraEAN) {
		this.barraEAN = barraEAN;
	}

	public String getBarraUPC() {
		return BarraUPC;
	}

	public void setBarraUPC(String barraUPC) {
		BarraUPC = barraUPC;
	}

	public String getIdUnicoArt() {
		return idUnicoArt;
	}

	public void setIdUnicoArt(String idUnicoArt) {
		this.idUnicoArt = idUnicoArt;
	}

	public String getMagazzino() {
		return magazzino;
	}

	public void setMagazzino(String magazzino) {
		this.magazzino = magazzino;
	}

	public String getRagstampe() {
		return ragstampe;
	}

	public void setRagstampe(String ragstampe) {
		this.ragstampe = ragstampe;
	}

	public String getTipoOrdine() {
		return tipoOrdine;
	}

	public void setTipoOrdine(String tipoOrdine) {
		this.tipoOrdine = tipoOrdine;
	}

	public String getColore() {
		return colore;
	}

	public void setColore(String colore) {
		this.colore = colore;
	}

}
